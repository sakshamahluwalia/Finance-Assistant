<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:id="loginGrid" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
    <children>
        <TextField fx:id="username" layoutX="284.0" layoutY="157.0"/>
        <PasswordField fx:id="password" layoutX="284.0" layoutY="198.0"/>
        <Button fx:id="LogInbtn" layoutX="255.0" layoutY="243.0" mnemonicParsing="false" onAction="#verify"
                text="Log In"/>
        <Label layoutX="184.0" layoutY="162.0" text="Username:"/>
        <Label layoutX="190.0" layoutY="204.0" prefHeight="16.0" prefWidth="71.0" text="Password:"/>
    </children>
</AnchorPane>
