<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="loginGrid" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
    <children>
        <TextField fx:id="username" layoutX="284.0" layoutY="157.0" />
        <PasswordField fx:id="password" layoutX="284.0" layoutY="198.0" />
        <Button fx:id="LogInbtn" layoutX="526.0" layoutY="358.0" mnemonicParsing="false" onAction="#verify" text="Log In" />
        <Label layoutX="184.0" layoutY="162.0" text="Username:" />
        <Label layoutX="190.0" layoutY="204.0" prefHeight="16.0" prefWidth="71.0" text="Password:" />
      <Button fx:id="LoadTransactions" layoutX="181.0" layoutY="247.0" mnemonicParsing="false" onAction="#loadTransactions" text="Load Transactions" />
      <Button fx:id="LoadNetWorth" layoutX="338.0" layoutY="247.0" mnemonicParsing="false" onAction="#loadNetWorth" text="Load Net worth" />
    </children>
</AnchorPane>
